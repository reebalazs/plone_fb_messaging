<tal:head tal:condition="view/auth_token">
    <script src="https://cdn.firebase.com/v0/firebase.js"
        type="text/javascript"></script>
    <meta name="fb-messaging-auth-token"
        tal:attributes="content view/auth_token">
    <meta name="fb-messaging-url"
        tal:attributes="content view/config/firebase_url">
    <meta name="fb-messaging-static"
        tal:attributes="content string:${context/@@plone_portal_state/portal_url}/++resource++fb_messaging/">
</tal:head>